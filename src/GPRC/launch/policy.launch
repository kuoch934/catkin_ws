<launch>

    <!-- Policy node -->
    <node pkg="GPRC" type="policy.py" name="policy_node" output="screen">
        <!-- action -->
        <param name="scale" value="0.5"/>
        <rosparam param="default_joint_pos">
            [0.0, 0.3, -0.6, 0.3, -0.6, 0.0,
            0.3, -0.6, 0.3, -0.6, 0.0, 0.0,
            0.1, -0.2, 0.0, 0.2, -0.1, 0.0, 0.0]
        </rosparam>
    </node>

    <!-- rosserial node -->
